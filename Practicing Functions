// A function can return any number of results. 

package main

import "fmt"

func add(x int, y int) int {
	return x + y
}


//naked return in function
func split(sum int) (x, y int) {
	x = sum * 4 / 9
	y = sum - x
	return 
}



func main() {
	// fmt.Println(add(12, 14))
	fmt.Println(split(17))  
}








