package main

import (
	"fmt"
)

var (
	i int = 24
)

func main() {
	fmt.Println(i)
	fmt.Printf("%T", i)
}



//Scope to the package (lower case at the package level)
// golabal variable (upper case at the package level)
// block scope (inside to the function)


