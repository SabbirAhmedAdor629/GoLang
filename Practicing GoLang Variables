package main

import (
	"fmt"
)

var (
	i int = 24
)

func main() {
	var j float32
	j = float32(i)

	fmt.Println(j)
	fmt.Printf("%T", i)
	fmt.Printf("\n")
	fmt.Printf("%T", j)

	var k string
	k = stringconv.Itoa(j)
	fmt.Println(j)

}

