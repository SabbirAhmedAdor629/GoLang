
package main

import "fmt"

func main() {
	fmt.Println("Hello, 世界")

	//Variables
	var j int = 27
	fmt.Printf("%v, %T", j, j)
	fmt.Printf("\n")

	var i int
	i = 27
	k := 28
	fmt.Println(i, k)
}





