













package main

import "fmt"

// one of the use case of enumerated constant (iota)

const (
	errorSpecialist = iota
	catSpecialist
	dogSpecialist
	snakeSpecialist
)

func main() {
	var specialistType int
	fmt.Println(specialistType == catSpecialist)
}






package main

import "fmt"

const myConst int16 = 211

const (
	a = iota
	b = iota
	c = iota
	d
	e
)

func main() {
	// Constants
	// Constants can be Shadowed (both value and type)
	// iota : for enumerated valus
	const myConst int = 27
	fmt.Println(myConst)
	fmt.Println(a)
	fmt.Println(b)
	fmt.Println(c)
	fmt.Println(d)
	fmt.Println(e)

}
